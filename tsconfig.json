{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom"
    ],
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES2016",
    "typeRoots": [
      "node_modules/@types/",
      "typings/index.d.ts"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
